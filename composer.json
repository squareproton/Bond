{
    "name": "squareproton/bond",
    "type": "library",
    "description": "Postgres ORM and other misc php framework goodness.",
    "keywords": ["Postgres", "ORM", "Framework", "Normality"],
    "homepage": "",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Peter Tuthill",
            "email": "squareproton@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": {
            "Bond\\": "src/",
            "SebastianBergmann\\": "lib/"
        },
        "files": [
            "src/Bond/functions.php",
            "lib/php-ref/ref.php",
            "lib/underscore.php",
            "lib/php-ref-config.php"
        ]
    },
    "require": {
        "php": ">=5.5",
        "symfony/dependency-injection": "dev-master",
        "symfony/proxy-manager-bridge": "dev-master",
        "symfony/yaml": "dev-master",
        "symfony/config": "dev-master",
        "debugchannel/php-client": "0.2"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    },    
    "prefer-stable": true,
    "config": {
        "process-timeout": 5000
    }
}
